{
    "title": "GitBook Handbook",
    "author": "feihu han <hfhfn@sina.com>",
    "description": "This is a sample book created by gitbook",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "extension": null,
    "generator": "site",
    "structure":
    {
        "readme": "README.md"
    },
    "links":
    {
        "sharing":
        {
            "all": null,
            "facebook": null,
            "google": null,
            "twitter": null,
            "weibo": null
        },
        "sidebar":
        {
            "Gitbook docs": "https://smoothies.com.cn/gitbook-docs/Gitbook/1_Gitbook_Get_Started.html"
        }
    },
    "output": null,
    "pdf":
    {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin":
        {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": true,
        "paperSize": "a4"
    },
    "plugins":
    [
        "theme-comscore",
        "-sharing",
        "sharing-plus",
        "-lunr",
        "-search",
        "search-pro",

        "mathjax",
        "mathjax-pro",
        "-katex",
        "-copy-code-button",
        "code",
        "klipse",
        "-highlight",
        "prism",

        "pageview-count",
        "auto-scroll-table",
        "popup",
        "custom-favicon",
        "scroll-to-top",
        "anchors",
        "insert-logo",
        "todo",

        "expandable-chapters-small",
        "back-to-top-button",
        "tbfed-pagefooter",
        "-page-toc-button",
        "anchor-navigation-ex",
        "page-treeview",
        "splitter"
    ],
    "pluginsConfig":
    {
        "anchor-navigation-ex": {
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right",
            "multipleH1": false,
            "multipleH2": false,
            "multipleH3": false,
            "multipleH4": false,
            "showLevelIcon": false,
            "showLevel": false
        },
        "page-toc-button": 
        {
            "maxTocDepth": 2,
            "minTocSize": 2
        },
        "page-treeview": 
        {
            "copyright": "",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        },
        "insert-logo": 
        {
            "url": "img/self_logo.jpg",
            "style": "background: none; max-height: 30px; min-height: 30px"
        },
        "favicon": "img/self_logo.jpg",
        "prism": 
        {
            "css": 
            [
            "prismjs/themes/prism-tomorrow.css"
            ]
        },
        "mathjax-pro":
        {
            "forceSVG": true
        },
        "sharing":
        {
            "douban": false,
            "facebook": false,
            "google": true,
            "pocket": false,
            "qq": false,
            "qzone": true,
            "twitter": false,
            "weibo": true,
            "all":
            [
                "douban",
                "facebook",
                "google",
                "instapaper",
                "linkedin",
                "twitter",
                "weibo",
                "messenger",
                "qq",
                "qzone",
                "viber",
                "whatsapp"
            ]
        },
        "tbfed-pagefooter":
        {
            "copyright": "Copyright &copy mine 2000-2020",
            "modify_label": "文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    },
    "variables":
    {}
}